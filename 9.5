#include <iostream>
#include <cstdlib>
#include <ctime>
using namespace std;

int main() {
    int randomMassive[5];
    int max = -1;     
    int second = -1;

    srand(time(0));
    cout << "Massive before:\n";

    for (int i = 0; i < 5; i++) {
        randomMassive[i] = rand() % 10; 
        cout << randomMassive[i] << " ";
    }

    cout << endl;

    for (int i = 0; i < 5; i++) {
        if (randomMassive[i] > max) {
            second = max;
            max = randomMassive[i];
        }
        else if (randomMassive[i] > second && randomMassive[i] != max) {
            second = randomMassive[i];
        }
    }

    cout << "Max number: " << max << endl;
    cout << "Second max: " << second << endl;

    return 0;
}
